{
  "name": "hire-task",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "start:server": "ts-node ./server/index.ts",
    "start:web": "webpack serve",
    "start": "run-p start:*",
    "eslint": "eslint src/",
    "style": "npx stylelint **/*.scss",
    "prettier": "npx prettier src/ --check",
    "format": "npx prettier src/ --write & npx stylelint **/*.scss --fix & eslint src/ --fix"
